<% content_for :head do %>
    <script type="text/javascript">
      var coordinates = null;
      $(function() {
      <% if !@master.longitude.blank? and !@master.latitude.blank? %>
        coordinates = <%= "[#{@master.longitude},#{@master.latitude}]" %>;
      <% end %>
        init(coordinates);
      });
    </script>
<% end %>