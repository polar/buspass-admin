<%=
    begin
      site = @site
      page = site.pages.find_by_full_path("/all-websites")
      page.render(self, :status => 200, :content_type => 'text/html')
    rescue => boom
      logger.detailed_error(boom)
      render :text => I18n.t('cms.content.page_not_found'), :status => 404
    end
%>