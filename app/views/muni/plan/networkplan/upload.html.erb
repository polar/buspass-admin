<h2>Network <%= @network.name %></h2>
<h2>Upload File</h2>
<%= form_for @network,
             # Cannot use :network here, it's already taken'
             :as => @network_param_name,
             :url => plan_networkplan_path(:network => @network, :muni => @muni.slug),
             :html => {:multipart => true} do |f| %>
  <p>
    <%= f.label :upload_file, "Zip File" %>
    <%= f.file_field :upload_file %>
    <%= f.hidden_field :upload_file_cache %>
  </p>
  <p><%=  f.submit %></p>
<% end %>