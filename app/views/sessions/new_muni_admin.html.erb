<% content_for :left do %>
    <%=  render :partial => "navigation/admin_nav" %>
<% end %>

<h2>Sign in</h2>

<% for provider in @providers do %>
    <a href="/auth/<%= provider %>" class="auth_provider">
      <%= image_tag "auth/#{provider}_64.png", :size => "64x64", :alt => "#{provider.titleize}" %>
      <%= provider.titleize %>
    </a>
<% end %>
<div class="clear"></div>
