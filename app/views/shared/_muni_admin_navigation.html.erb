<div class="user_nav">

    <% if muni_admin_signed_in? %>
      <%= link_to('Logout',
                  destroy_muni_admin_session_path(:master_id => @master.id),
                  :method=>'delete',
                  :class => "logout") %>
    <% else %>
      <%= link_to('Login',
                  new_muni_admin_session_path(:master_id => @master.id),
                  :class => "login")  %>
    <% end %>

    <% if muni_admin_signed_in? %>
      <%= link_to('Edit account',
                  edit_muni_admin_registration_path(:master_id => @master.id),
                  :class => "edit_account") %>
    <% else %>
      <%= link_to('Sign up',
                  new_muni_admin_registration_path(:master_id => @master.id),
                  :class => "sign_up")  %>
    <% end %>

    <%= link_to(current_muni_admin.name,
                edit_muni_admin_registration_path(:master_id => @master.id),
                :class => "name") if current_muni_admin %>

</div>