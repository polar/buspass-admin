<div class="user_nav">

  <% if current_muni_admin %>
      <span class="signed-in">
        <div>
            <%= link_to current_muni_admin.name,
                  edit_registration_master_muni_admins_path(:master_id => @master.id) %>
            <%= link_to 'Logout',
                  destroy_muni_admin_sessions_path(:master_id => @master.id),
                  :method=>'delete' %>
        </div>
        <%= image_tag("silhouette36.png") %>
      </span>
  <% else %>
      <span class="signed-out">
          <%= link_to('Sign in', customer_sign_in_path, :class => "btn") %>
      </span>
  <% end %>


</div>