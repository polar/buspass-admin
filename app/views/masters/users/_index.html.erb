<h1>Users</h1>
<p></p>
<%= form_tag master_users_path(@master), :method => 'get', :id => "users_search", :class => "form-search" do %>
    <p>
      <%= text_field_tag :search, params[:search], :class => "search-query" %>
      <%= submit_tag "Search", :name => nil, :class => "btn" %>
    <div id="user_params">
      <%= render :partial => "masters/users/search_params" %>
    </div>
    </p>
<% end %>

<div id="users"><%= render :partial => "masters/users/users" %></div>
