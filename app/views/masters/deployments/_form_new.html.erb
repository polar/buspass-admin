
<%= form_for(@deployment, :url => master_deployments_path(@master), :validate => true) do |f| %>
    <%= error_messages!(@deployment) %>

    <div><%= f.label :name, "Internal Name" %>
      <%= f.text_field :name %></div>

    <div><%= f.label :note, "Note" %>
      <%= f.text_area :note %></div>

    <div><%= f.submit "Create", :class => "btn"  %></div>
<% end %>