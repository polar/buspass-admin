<table class="table table-striped">
  <thead><tr>
    <th>Route</th>
    <th>Start Date</th>
    <th>End Date</th>
    <th>Day Class</th>
    <th>Vehicle Journeys</th>
  </tr></thead>
  <tbody><tr>
    <td><%=  link_to @service.route.code, master_deployment_network_route_path(@master, @deployment, @network, @vehicle_journey.route)  %></td>
    <!--- TODO: Internatialize Time -->
    <td><%=  @service.operating_period_start_date.strftime(@master.date_format) %></td>
    <td><%=  @service.operating_period_end_date.strftime(@master.date_format) %></td>
    <td><%=  @service.day_class %></td>
    <td><%=  @service.operating_period_exception_dates.map {|d| "<span>#{d.strftime(@master.date_format)}</span>"}.join("") %></td>
    <td><%=  @service.vehicle_journeys.count %></td>
  </tr></tbody>
</table>