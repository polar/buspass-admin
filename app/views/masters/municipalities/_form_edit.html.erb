
<%= form_for(@municipality, :url => master_municipality_path(@master), :validate => true) do |f| %>
    <%= error_messages!(@municipality) %>

    <%= f.label :name, "Internal Name" %>
    <%= f.text_field :name %>

    <%= f.label :note, "Note" %>
    <%= f.text_area :note %>

    <div><%= f.submit "Update", :class=>"btn"%></div>
<% end %>
