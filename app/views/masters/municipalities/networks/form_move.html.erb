
<%=  form_for( @network, :url => moveto_master_municipality_network_path(@master, @municipality, @network)) do |f| %>

    Move <%= link_to(@network.name, master_municipality_network_path(@master, @municipality, @network)) %> to
    <%= f.collection_select(:municipality, @municipalities, :id, :name, :disabled => @disabled_options) %>
    <%= f.submit "Copy", :disabled => @disabled_options.reduce(false) {|t,v| t || v } %>
<% end %>