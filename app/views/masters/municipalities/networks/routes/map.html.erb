<%=
    begin
      site = @master.admin_site
      page = site.pages.find_by_full_path("/deployments/#{@municipality.slug}/networks/#{@network.slug}/routes/#{@route.slug}/map")
      page.render(self, :status => 200, :content_type => 'text/html')
    rescue Exception => boom
      logger.detailed_error(boom)
      render :text => I18n.t('cms.content.page_not_found'), :status => 404
    end
%>