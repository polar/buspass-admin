<table class="table table-striped">
  <tr>
    <th>Route</th>
    <th>Start Date</th>
    <th>End Date</th>
    <th>Day Class</th>
    <th>Vehicle Journeys</th>
  </tr>
  <tr>
    <td><%=  link_to @service.route.code, master_municipality_network_route_path(@master, @municipality, @network, @vehicle_journey.route)  %></td>
    <!--- TODO: Internatialize Time -->
    <td><%=  @service.operating_period_start_date.strftime("%m-%d-%Y") %></td>
    <td><%=  @service.operating_period_end_date.strftime("%m-%d-%Y") %></td>
    <td><%=  @service.day_class %></td>
    <td><%=  @service.vehicle_journeys.count %></td>
  </tr>
</table>