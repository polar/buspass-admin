<table>
  <tr>
    <th>Route</th>
    <th>Network</th>
  </tr>
  <% for route in @routes do %>
      <tr>
        <td><%=  link_to route.code, master_municipality_network_route_path(@master, @municipality, @network, route)  %></td>
        <td><%=  link_to route.network.name, master_municipality_network_path(@master, @municipality, route.network)  %></td>
      </tr>
  <% end %>
</table>