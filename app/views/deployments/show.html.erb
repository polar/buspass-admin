<head>
  <script type="text/javascript">
      var activePlanView;
      $(function () {
          $("body").layout({ applyDefaultStyles:true });

          activePlanView = new BusPass.ActivePlanBasketController();

          activePlanView.activePlanView("#activePlanView", {
              loginUrl : '<%= @loginUrl %>'
          });

          $("#deployment-clock").clock({format : 24});

          // Start the show!
          activePlanView.onCreate();
          activePlanView.onStart();
          activePlanView.onResume();
      });
  </script>
</head>

  <div class="row">
    <div class="span12">
      <div id="deployment-header"><%= @master.name %></div><div id="deployment-clock"></div>
    </div>
  </div>

  <%=  render :partial => "shared/webmap_active_plan" %>
